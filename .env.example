# Microsoft Teams Webhook URL (opcional)
# Para receber alertas quando há mudanças nas APIs
TEAMS_WEBHOOK_URL=

# AI Gateway Configuration (opcional)
# Para gerar resumos inteligentes das mudanças
AI_GATEWAY_URL=
AI_API_KEY=

# Prometheus Metrics Port (opcional)
# Porta para expor métricas Prometheus
METRICS_PORT=9090

# Email Configuration (opcional)
# Para receber alertas por email quando há mudanças
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=seu-email@gmail.com
SMTP_PASS=sua-senha-de-app
SMTP_FROM=seu-email@gmail.com
EMAIL_TO=destinatario@exemplo.com
